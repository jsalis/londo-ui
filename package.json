{
  "name": "londo-ui",
  "version": "0.1.24",
  "description": "The spectacular view. A component library for React web apps.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-components",
    "dark-mode",
    "ui-library",
    "londo-ui"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build": "npm run clean && npm run build-js && npm run build-ts",
    "build-storybook": "build-storybook",
    "build-js": "cross-env NODE_ENV=production babel src --ignore src/stories --extensions \".ts,.tsx\" --out-dir dist",
    "build-ts": "tsc --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "lint": "eslint --fix src",
    "test": "jest",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": "prettier --write",
    "*.{ts,tsx}": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "framer-motion": "^6.5.1",
    "react-focus-lock": "^2.9.1",
    "react-remove-scroll": "^2.5.5",
    "styled-system": "^5.1.5",
    "tinycolor2": "^1.4.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "styled-components": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/react": "^6.4.17",
    "@storybook/theming": "^6.4.17",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@types/styled-system": "^5.1.15",
    "@types/tinycolor2": "^1.4.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "csstype": "^3.0.10",
    "eslint": "^8.6.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsalis/londo-ui.git"
  },
  "author": "jsalis <johnsalis@live.com>",
  "license": "MIT"
}
