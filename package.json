{
  "name": "londo-ui",
  "version": "0.1.6",
  "description": "The spectacular view. A component library for React web apps.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "!dist/stories"
  ],
  "keywords": [
    "react",
    "react-components",
    "dark-mode",
    "ui-library",
    "londo-ui"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm run clean && NODE_ENV=production babel src --out-dir dist --copy-files --ignore **/*.stories.jsx",
    "prepare": "husky install",
    "lint": "eslint --fix src",
    "test": "jest",
    "clean": "rimraf dist",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,jsx,css,json,md}": "prettier --write",
    "*.{js,jsx}": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "prop-types": "^15.7.2",
    "styled-system": "^5.1.5",
    "tinycolor2": "^1.4.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "styled-components": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "styled-components": "^5.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsalis/londo-ui.git"
  },
  "author": "jsalis <johnsalis@live.com>",
  "license": "MIT"
}
